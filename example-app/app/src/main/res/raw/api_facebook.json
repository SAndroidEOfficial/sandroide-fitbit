{
  "auth":{
    "type":"oauth2",
    "url":"https://www.facebook.com/v2.8/dialog/oauth?client_id=[client_id]&redirect_uri=[redirect_uri]&response_type=token"
  },
  "userfeed":{
    "url":"https://graph.facebook.com/v2.8/[user_id]/feed",
    "headers":{
      "Authorization":"OAuth [auth.access_token]"
    }
  },
  "me":{
    "url":"https://graph.facebook.com/v2.8/me",
    "headers":{
      "Authorization":"OAuth [auth.access_token]"
    }
  }
}